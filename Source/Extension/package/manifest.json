{
    "name": "SmartNoise",
    "version": "1.0.0.0",
    "manifest_version": 3,
    "description": "",
    "icons": {
        "16": "icons/icon16x16.png",
        "32": "icons/icon32x32.png",
        "64": "icons/icon64x64.png",
        "128": "icons/icon128x128.png"
    },
    "action": {
        "default_popup": "configuration/index.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["browser/index.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "web_accessible_resources": [{
        "resources": [
            "images/*.png"
        ],
        "matches": [],
        "extension_ids": []
    }]
}
