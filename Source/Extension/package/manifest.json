{
    "name": "SmartNoise",
    "version": "1.0.0.0",
    "manifest_version": 2,
    "description": "",
    "icons": {
        "16": "icons/icon16x16.png",
        "32": "icons/icon32x32.png",
        "64": "icons/icon64x64.png",
        "128": "icons/icon128x128.png"
    },
    "browser_action": {
        "default_popup": "configuration/index.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["browser/index.js"]
        }
    ],
    "web_accessible_resources": [
        "images/*.png"
    ]
}
